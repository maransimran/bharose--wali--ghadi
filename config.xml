<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bharose.ghadi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>भरोसे वाली घड़ी</name>
    <description>A reliable alarm clock app.</description>
    <author>Simran</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="AndroidXEnabled" value="true" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
</widget>